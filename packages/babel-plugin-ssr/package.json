{
  "name": "@impress/babel-plugin-ssr",
  "version": "0.0.1",
  "description": "Babel plugin for automatic SSR",
  "repository": {
    "url": "https://github.com/betula/impress"
  },
  "author": "Viacheslav Bereza <mail@betula.co>",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "files": [
    "/release",
    "/src",
    "/README.md"
  ],
  "scripts": {
    "build": "npx tsc --build tsconfig.release.json",
    "test": "npx jest --coverage",
    "test-with-coveralls": "npx jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "npx eslint ./src --ext ts",
    "clean": "npx rimraf ./release && npx rimraf ./coverage",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "devDependencies": {
    "@babel/core": "~7.8.3",
    "@types/jest": "~24.0.25",
    "typescript": "~3.7.4",
    "eslint": "~6.8.0",
    "@typescript-eslint/eslint-plugin": "~2.16.0",
    "@typescript-eslint/parser": "~2.16.0",
    "jest": "~24.9.0",
    "ts-jest": "~24.3.0",
    "coveralls": "~3.0.9"
  }
}

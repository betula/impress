{
  "name": "@impress/babel-plugin-ssr",
  "version": "0.3.0",
  "description": "Babel plugin for Impress automatic SSR",
  "repository": {
    "url": "https://github.com/betula/impress"
  },
  "author": "Viacheslav Bereza <mail@betula.co>",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "files": [
    "/release",
    "/src",
    "/README.md"
  ],
  "scripts": {
    "build": "npx rollup -c",
    "dev": "npx rollup -cw",
    "test": "npx jest --coverage",
    "test-with-coveralls": "npx jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "npx eslint ./src --ext ts && npx eslint ./tests --ext ts",
    "clean": "npx rimraf ./release && npx rimraf ./coverage"
  },
  "dependencies": {
    "@babel/core": "~7.8.3"
  },
  "devDependencies": {
    "@types/jest": "~24.0.25",
    "@types/node": "^13.5.1",
    "@typescript-eslint/eslint-plugin": "~2.16.0",
    "@typescript-eslint/parser": "~2.16.0",
    "coveralls": "~3.0.9",
    "eslint": "~6.8.0",
    "jest": "~24.9.0",
    "ts-jest": "~24.3.0",
    "typescript": "~3.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}

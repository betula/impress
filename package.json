{
  "name": "@impress/react",
  "version": "0.3.2",
  "description": "Modular Data-View binding mechanism for React",
  "repository": {
    "url": "https://github.com/betula/impress"
  },
  "source": "./src/index.ts",
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "files": [
    "/release",
    "/src",
    "/jest-cleanup-after-each.js",
    "/README.md"
  ],
  "scripts": {
    "build": "npx rollup -c",
    "dev": "npx rollup -cw",
    "test": "npx jest --coverage",
    "test-with-coveralls": "npx jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "npx eslint ./src --ext ts && npx eslint ./tests --ext ts,tsx",
    "clean": "npx rimraf ./release && npx rimraf ./coverage",
    "lerna:update": "npx lerna bootstrap",
    "lerna:bootstrap": "npm i && npx lerna bootstrap && npm run lerna:build && npm run lerna:test",
    "lerna:build": "npx lerna run build",
    "lerna:test": "npx lerna run test",
    "lerna:lint": "npx lerna run lint",
    "lerna:clean": "npx lerna run clean",
    "lerna:publish": "npm run lerna:clean && npm run lerna:lint && npm run lerna:build && npm run lerna:test && npx lerna publish"
  },
  "keywords": [
    "react store",
    "data view binding",
    "data-view",
    "data binding",
    "state-manager",
    "state manager",
    "state",
    "store",
    "flux"
  ],
  "author": "Viacheslav Bereza <mail@betula.co>",
  "license": "MIT",
  "browser": {
    "async_hooks": false
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "typescript": "^3.7.5",
    "rollup": "^1.31.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "@zerollup/ts-transform-paths": "^1.7.11",
    "rollup-plugin-terser": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "coveralls": "^3.0.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "lerna": "^3.20.2"
  },
  "publishConfig": {
    "access": "public"
  }
}

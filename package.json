{
  "name": "@impress/react",
  "version": "2.7.0",
  "description": "Modular Data-View binding mechanism for React",
  "repository": {
    "url": "https://github.com/betula/impress"
  },
  "bugs": {
    "url": "https://github.com/betula/impress/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "npx rollup -c",
    "dev": "npx rollup -cw",
    "test": "npx jest --coverage",
    "lint": "npx eslint ./src --ext ts && npx eslint ./tests --ext ts,tsx",
    "clean": "npx rimraf ./release && npx rimraf ./coverage",
    "lerna:reset": "npm run lerna:clean && npx lerna clean --yes && npm run lerna:bootstrap",
    "lerna:update": "npx lerna bootstrap --no-ci",
    "lerna:bootstrap": "npx lerna bootstrap --no-ci && npm run lerna:build && npm run lerna:test && npm run lerna:lint",
    "lerna:build": "npx lerna run build",
    "lerna:test": "npx lerna run test",
    "lerna:lint": "npx lerna run lint",
    "lerna:clean": "npx lerna run clean",
    "lerna:publish": "npm run lerna:clean && npm run lerna:lint && npm run lerna:build && npm run lerna:test && npx lerna publish",
    "site:build": "npx marked README.md -o index.html"
  },
  "author": "Viacheslav Bereza <mail@betula.co> (http://betula.co)",
  "maintainers": [
    "Viacheslav Bereza <mail@betula.co> (http://betula.co)"
  ],
  "contributors": [
    "Sergey Sova <mail@sergeysova.com> (https://sergeysova.com)"
  ],
  "keywords": [
    "react store",
    "data view binding",
    "data-view",
    "data binding",
    "state-manager",
    "state manager",
    "state",
    "store",
    "flux"
  ],
  "source": "./src/index.ts",
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "files": [
    "/release",
    "/src",
    "/jest-cleanup-after-each.js",
    "/README.md"
  ],
  "browser": {
    "async_hooks": false
  },
  "peerDependencies": {
    "react": ">=15.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@zerollup/ts-transform-paths": "^1.7.11",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^1.31.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
